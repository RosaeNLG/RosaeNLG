{
  "name": "rosaenlg",
  "description": "A complete Natural Language Generation library based on Pug",
  "version": "1.20.2",
  "keywords": [
    "NLG",
    "Natural Language Generation"
  ],
  "author": "Ludan Stoeckl√© <ludan.stoeckle@rosaenlg.org>",
  "maintainers": [],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/RosaeNLG/rosaenlg.git"
  },
  "bugs": {
    "url": "https://github.com/RosaeNLG/rosaenlg/issues"
  },
  "homepage": "https://rosaenlg.org",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "grammars": "gulp --gulpfile gulpfile.js/grammars.js all",
    "build:before": "gulp --gulpfile gulpfile.js/init.js init && gulp --gulpfile gulpfile.js/copyAndChangeVersion.js forIndexJs && cp src/ExoticInterface.d.ts dist/",
    "build:mainpug": "echo {} > ../rosaenlg-pug-code-gen/dist/compiledMain_client.json && echo {} > ../rosaenlg-pug-code-gen/dist/compiledMain_server.json && gulp mainpug",
    "build:tsc": "node ../_helpers/fake_resources.js before_tsc && tsc && node ../_helpers/fake_resources.js after_tsc && gulp --gulpfile gulpfile.js/copyAndChangeVersion.js forNlgLib",
    "build:main": "npm run grammars && npm run build:tsc && npm run build:mainpug",
    "build:after": "npm run rollup",
    "build": "npm run build:before && npm run build:main && npm run build:after",
    "doc": "gulp doc",
    "doc:publish": "gulp doc_s3",
    "test:tiny": "node --max-old-space-size=2048 ../../node_modules/mocha/bin/mocha test/test-rosaenlg/tiny.js",
    "test": "npm run test:tiny && nyc --reporter=lcov --reporter=text mocha --recursive --exclude test/test-rosaenlg/tiny.js",
    "rollup": "node ../_helpers/fake_resources.js before_rollup && rollup --config --es --nocomp && rollup --config --de --nocomp && rollup --config --fr --nocomp && rollup --config --en --nocomp && rollup --config --it --nocomp && rollup --config --OTHER --nocomp && rollup --config --es --comp && rollup --config --de --comp && rollup --config --fr --comp && rollup --config --en --comp && rollup --config --it --comp && rollup --config --OTHER --comp && node ../_helpers/fake_resources.js after_rollup",
    "rollup_fr": "node ../_helpers/fake_resources.js before_rollup && rollup --config --fr --comp && rollup --config --fr --nocomp && node ../_helpers/fake_resources.js after_rollup"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/",
      "dist/index.js",
      "examples",
      "doc",
      "lib",
      "register.js",
      "support",
      "dist/browser",
      "dist/*-grammar.js"
    ],
    "check-coverage": true,
    "statements": 95,
    "lines": 95,
    "functions": 95,
    "branches": 95,
    "watermarks": {
      "statements": [
        60,
        95
      ],
      "lines": [
        60,
        95
      ],
      "functions": [
        60,
        95
      ],
      "branches": [
        60,
        95
      ]
    }
  },
  "dependencies": {
    "@types/numeral": "^0.0.28",
    "english-determiners": "2.20.2",
    "english-ordinals": "0.6.2",
    "english-plurals": "0.12.2",
    "english-plurals-list": "0.12.2",
    "english-verbs-gerunds": "0.12.2",
    "english-verbs-helper": "0.12.2",
    "english-verbs-irregular": "0.12.2",
    "french-adjectives-wrapper": "0.6.2",
    "french-determiners": "2.20.2",
    "french-ordinals": "2.20.2",
    "french-verbs": "2.20.2",
    "french-verbs-lefff": "0.13.2",
    "french-words": "0.6.2",
    "french-words-gender-lefff": "0.13.2",
    "german-adjectives": "2.20.2",
    "german-adjectives-dict": "0.13.2",
    "german-determiners": "2.20.2",
    "german-dict-helper": "2.20.2",
    "german-ordinals": "2.20.2",
    "german-verbs": "2.20.2",
    "german-verbs-dict": "0.13.2",
    "german-words": "2.20.2",
    "german-words-dict": "0.13.2",
    "italian-adjectives": "2.20.2",
    "italian-adjectives-dict": "0.13.2",
    "italian-determiners": "2.20.2",
    "italian-ordinals-cardinals": "2.20.2",
    "italian-verbs": "2.20.2",
    "italian-verbs-dict": "0.13.2",
    "italian-words": "2.20.2",
    "italian-words-dict": "0.13.2",
    "lefff-helper": "2.20.2",
    "date-fns": "^2.16.1",
    "morph-it-helper": "2.20.2",
    "rosaenlg-n2words": "^0.0.1",
    "numeral": "^2.0.6",
    "ordinal-spanish": "^0.1.3",
    "pug-runtime": "^2.0.4",
    "pug-strip-comments": "^1.0.3",
    "random-js": "^2.1.0",
    "rosaenlg-commons": "0.4.2",
    "rosaenlg-filter": "2.20.2",
    "rosaenlg-pug-code-gen": "3.20.2",
    "rosaenlg-pug-lexer": "5.19.2",
    "rosaenlg-pug-linker": "4.19.2",
    "rosaenlg-pug-load": "3.19.2",
    "rosaenlg-pug-parser": "6.19.2",
    "rosaenlg-yseop": "2.20.2",
    "spanish-adjectives-wrapper": "0.6.2",
    "spanish-determiners": "0.6.2",
    "spanish-verbs-wrapper": "0.6.2",
    "spanish-words": "0.6.2",
    "synonym-optimizer": "2.20.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.1",
    "rollup-plugin-license": "^2.2.0",
    "@types/node": "^14.11.2",
    "browserify": "^16.5.0",
    "gulp-concat": "^2.6.1",
    "gulp-pegjs": "^0.2.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "recursive-readdir-sync": "^1.0.6",
    "rollup": "^2.28.2",
    "rollup-plugin-analyzer": "^3.2.2",
    "rollup-plugin-modify": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-unassert": "^0.3.0",
    "source-map-support": "^0.5.16",
    "stream": "^0.0.2"
  },
  "browser": {
    "fs": false
  }
}
