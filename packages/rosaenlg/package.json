{
  "name": "rosaenlg",
  "description": "A complete Natural Language Generation library based on Pug",
  "version": "1.5.8",
  "keywords": [
    "NLG",
    "Natural Language Generation"
  ],
  "author": "Ludan Stoeckl√© <ludan.stoeckle@rosaenlg.org>",
  "maintainers": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:rosaenlg-projects/rosaenlg.git"
  },
  "bugs": {
    "url": "https://gitlab.com/rosaenlg-projects/rosaenlg/issues"
  },
  "homepage": "https://rosaenlg.org",
  "main": "dist/index.js",
  "scripts": {
    "grammars": "gulp --gulpfile gulpfile.js/grammars.js all",
    "browserify": "gulp browser_it_IT && gulp browser_it_IT_compile && gulp browser_de_DE && gulp browser_de_DE_compile && gulp browser_fr_FR && gulp browser_fr_FR_compile && gulp browser_en_US && gulp browser_en_US_compile && gulp browser_OTHER && gulp browser_OTHER_compile",
    "build:before": "gulp --gulpfile gulpfile.js/init.js init && cp lib/index.js dist/ && cp src/ExoticInterface.d.ts dist/",
    "build:main": "npm run grammars && tsc && gulp mainpug",
    "build:after": "npm run browserify",
    "build": "npm run build:before && npm run build:main && npm run build:after",
    "doc": "gulp doc",
    "doc:publish": "gulp doc_s3",
    "test": "nyc mocha",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/",
      "dist/index.js",
      "examples",
      "doc",
      "lib",
      "register.js",
      "support",
      "dist/browser",
      "dist/*-grammar.js"
    ],
    "check-coverage": true,
    "statements": 95,
    "lines": 95,
    "functions": 95,
    "branches": 95,
    "watermarks": {
      "statements": [
        60,
        95
      ],
      "lines": [
        60,
        95
      ],
      "functions": [
        60,
        95
      ],
      "branches": [
        60,
        95
      ]
    }
  },
  "dependencies": {
    "@types/numeral": "^0.0.26",
    "compromise": "^11.14.3",
    "debug": "^4.1.1",
    "english-determiners": "2.5.8",
    "french-adjectives": "2.5.8",
    "french-determiners": "2.5.8",
    "french-ordinals": "2.5.8",
    "french-verbs": "2.5.8",
    "french-words-gender": "2.5.8",
    "german-adjectives": "2.5.8",
    "german-determiners": "2.5.8",
    "german-dict-helper": "2.5.8",
    "german-ordinals": "2.5.8",
    "german-verbs": "2.5.8",
    "german-words": "2.5.8",
    "italian-adjectives": "2.5.8",
    "italian-determiners": "2.5.8",
    "italian-ordinals-cardinals": "2.5.8",
    "italian-verbs": "2.5.8",
    "italian-words": "2.5.8",
    "lefff-helper": "2.5.8",
    "moment": "^2.24.0",
    "morph-it-helper": "2.5.8",
    "numeral": "^2.0.6",
    "pluralize-fr": "^1.0.1",
    "pug-runtime": "^2.0.4",
    "pug-strip-comments": "^1.0.3",
    "random-js": "^2.1.0",
    "rosaenlg-filter": "2.5.8",
    "rosaenlg-pug-code-gen": "3.5.8",
    "rosaenlg-pug-filters": "4.5.8",
    "rosaenlg-pug-lexer": "5.4.8",
    "rosaenlg-pug-linker": "4.4.8",
    "rosaenlg-pug-load": "3.4.8",
    "rosaenlg-pug-parser": "6.4.8",
    "rosaenlg-yseop": "2.5.8",
    "synonym-optimizer": "2.5.8",
    "write-int": "^0.0.2",
    "written-number": "^0.9.1",
    "snyk": "^1.236.1"
  },
  "devDependencies": {
    "@types/node": "^12.12.7",
    "brfs": "^2.0.2",
    "browser-pack-flat": "^3.4.2",
    "browserify": "^16.5.0",
    "browserify-versionify": "^1.0.6",
    "common-shakeify": "^0.6.2",
    "envify": "^4.1.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-pegjs": "^0.2.0",
    "gulp-rename": "^1.4.0",
    "minify-stream": "^1.2.1",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "recursive-readdir-sync": "^1.0.6",
    "source-map-support": "^0.5.16",
    "typescript": "^3.7.2",
    "unassertify": "^2.1.1"
  },
  "browser": {
    "fs": false
  },
  "snyk": true
}
