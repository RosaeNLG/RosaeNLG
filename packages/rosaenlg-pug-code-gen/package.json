{
  "name": "rosaenlg-pug-code-gen",
  "version": "3.20.2",
  "description": "Default code-generator for pug. It generates HTML via a JavaScript template function. - RosaeNLG fork",
  "scripts": {
    "build": "node ../_helpers/fake_resources.js before_tsc && tsc && node ../_helpers/fake_resources.js after_tsc && cp lib/index.js dist/",
    "test": "nyc --reporter=lcov --reporter=text mocha"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "test/"
    ],
    "check-coverage": true,
    "statements": 95,
    "lines": 95,
    "functions": 95,
    "branches": 95,
    "watermarks": {
      "statements": [
        60,
        95
      ],
      "lines": [
        60,
        95
      ],
      "functions": [
        60,
        95
      ],
      "branches": [
        60,
        95
      ]
    }
  },
  "main": "dist/index.js",
  "keywords": [
    "pug"
  ],
  "devDependencies": {
    "@types/node": "^14.11.2",
    "source-map-support": "^0.5.16"
  },
  "dependencies": {
    "constantinople": "^4.0.1",
    "doctypes": "^1.1.0",
    "english-plurals": "0.12.2",
    "english-plurals-list": "0.12.2",
    "english-verbs-gerunds": "0.12.2",
    "english-verbs-helper": "0.12.2",
    "english-verbs-irregular": "0.12.2",
    "french-adjectives-wrapper": "0.6.2",
    "french-verbs": "2.20.2",
    "french-verbs-lefff": "0.13.2",
    "german-adjectives": "2.20.2",
    "german-adjectives-dict": "0.13.2",
    "german-verbs": "2.20.2",
    "german-verbs-dict": "0.13.2",
    "german-words": "2.20.2",
    "german-words-dict": "0.13.2",
    "italian-adjectives": "2.20.2",
    "italian-adjectives-dict": "0.13.2",
    "italian-verbs": "2.20.2",
    "italian-verbs-dict": "0.13.2",
    "italian-words": "2.20.2",
    "italian-words-dict": "0.13.2",
    "js-stringify": "^1.0.2",
    "pug-attrs": "^2.0.4",
    "pug-error": "^1.3.2",
    "pug-runtime": "^2.0.4",
    "recast": "^0.20.3",
    "spanish-adjectives-wrapper": "0.6.2",
    "spanish-verbs-wrapper": "0.6.2",
    "spanish-words": "0.6.2",
    "void-elements": "^3.1.0",
    "with": "^7.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RosaeNLG/rosaenlg.git"
  },
  "author": "Ludan Stoeckl√©, Forbes Lindesay",
  "license": "MIT"
}
